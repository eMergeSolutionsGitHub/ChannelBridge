<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="60dip">

        <RelativeLayout
            android:id="@+id/relLayout_listnavigation_main"
            android:layout_width="60dip"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true">

            <emerge.lk.channelbridge.Font.TextViewFontAwesome
                android:id="@+id/txtView__listnavigation_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:textColor="@color/appWhite"
                android:textSize="35sp" />
        </RelativeLayout>

        <emerge.lk.channelbridge.Font.FontTitle
            android:id="@+id/txtView__listnavigation_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/relLayout_listnavigation_main"
            android:layout_toRightOf="@+id/relLayout_listnavigation_main"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/appWhite" />
    </RelativeLayout>
