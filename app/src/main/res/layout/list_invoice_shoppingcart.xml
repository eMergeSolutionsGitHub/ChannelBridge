<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:id="@+id/linLayout_itinerary_item"
    xmlns:app="http://schemas.android.com/tools"
    android:layout_height="70dp">


    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/relLayout_invoices_products"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@color/background_one"
        android:layout_height="70dp"
        android:layout_marginTop="5dp">

        <RelativeLayout
            android:layout_height="match_parent"
            android:id="@+id/relLayout_list_invoice_customerimage"
            android:layout_centerVertical="true"
            android:layout_width="60dp"
            android:layout_marginLeft="5dp">

            <com.mikhaellopez.circularimageview.CircularImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imgView_list_invoiceshoppingcart_product"
                android:src="@drawable/user"
                />

        </RelativeLayout>

        <emerge.lk.channelbridge.Font.FontTitle
            android:text="TextView"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_toRightOf="@+id/relLayout_list_invoice_customerimage"
            android:layout_toEndOf="@+id/relLayout_list_invoice_customerimage"
            android:layout_width="120dp"
            android:textColor="@color/colorNavigationBar"
            android:layout_marginLeft="5dp"
            android:id="@+id/txtView_list_invoiceshoppingcart_productcode" />


        <emerge.lk.channelbridge.Font.FontTitle
            android:text="TextView"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_toRightOf="@+id/txtView_list_invoiceshoppingcart_productcode"
            android:layout_toEndOf="@+id/txtView_list_invoiceshoppingcart_productcode"
            android:textColor="@color/colorNavigationBar"
            android:layout_marginLeft="5dp"
            android:id="@+id/txtView_list_invoiceshoppingcart_productbatch"
            android:layout_width="120dp" />


        <emerge.lk.channelbridge.Font.FontTitle
            android:text="1000000"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_centerVertical="true"
            android:textColor="@color/colorNavigationBar"
            android:layout_toRightOf="@+id/txtView_list_invoiceshoppingcart_productbatch"
            android:layout_toEndOf="@+id/txtView_list_invoiceshoppingcart_productbatch"
            android:layout_marginLeft="5dp"
            android:id="@+id/txtView_list_invoiceshoppingcart_shelfqty"
            android:layout_width="80dp" />

        <emerge.lk.channelbridge.Font.FontTitle
            android:text="TextView"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/txtView_list_invoiceshoppingcart_shelfqty"
            android:layout_toEndOf="@+id/txtView_list_invoiceshoppingcart_shelfqty"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/colorNavigationBar"
            android:layout_marginLeft="5dp"
            android:id="@+id/txtView_list_invoiceshoppingcart_requestqty"
            android:layout_width="80dp" />

        <emerge.lk.channelbridge.Font.FontTitle
            android:text="TextView"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/txtView_list_invoiceshoppingcart_requestqty"
            android:layout_toEndOf="@+id/txtView_list_invoiceshoppingcart_requestqty"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/colorNavigationBar"
            android:layout_marginLeft="5dp"
            android:id="@+id/txtView_list_invoiceshoppingcart_orderqty"
            android:layout_width="80dp" />

        <emerge.lk.channelbridge.Font.FontTitle
            android:text="TextView"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/txtView_list_invoiceshoppingcart_orderqty"
            android:layout_toEndOf="@+id/txtView_list_invoiceshoppingcart_orderqty"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/colorNavigationBar"
            android:layout_marginLeft="5dp"
            android:id="@+id/txtView_list_invoiceshoppingcart_freeqty"
            android:layout_width="80dp" />

        <emerge.lk.channelbridge.Font.FontTitle
            android:text="TextView"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/txtView_list_invoiceshoppingcart_freeqty"
            android:layout_toEndOf="@+id/txtView_list_invoiceshoppingcart_freeqty"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/colorNavigationBar"
            android:layout_marginLeft="5dp"
            android:id="@+id/txtView_list_invoiceshoppingcart_discountqty"
            android:layout_width="80dp" />

        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true">

            <RelativeLayout
                android:layout_width="90dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_height="50dp"
                android:background="@color/colorGreen">

                <emerge.lk.channelbridge.Font.TextViewFontAwesome
                    android:text="@string/icon_dialog_edit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:textColor="@color/colorNavigationBar"
                    android:textSize="36sp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:background="@color/colorRed"
                android:layout_marginRight="10dp">

                <emerge.lk.channelbridge.Font.TextViewFontAwesome
                    android:text="@string/icon_invoice_product_delete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:textColor="@color/colorNavigationBar"
                    android:textSize="36sp" />
            </RelativeLayout>
        </RelativeLayout>


    </RelativeLayout>


</LinearLayout>