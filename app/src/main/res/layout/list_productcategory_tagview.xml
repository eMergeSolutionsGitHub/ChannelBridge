<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="50dp"
    android:background="@drawable/rounded_corner_productcategorytag"
    android:layout_marginTop="5dp">


    <android.support.constraint.ConstraintLayout
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="0dp"
        android:layout_width="50dp"
        android:layout_height="50dp">

        <emerge.lk.channelbridge.Font.TextViewFontAwesome
            android:text="@string/icon_invoiceproducttagview_product"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:textSize="30sp"
            tools:layout_editor_absoluteY="10dp"
            tools:layout_editor_absoluteX="10dp" />

    </android.support.constraint.ConstraintLayout>

    <emerge.lk.channelbridge.Font.FontTitle
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtView_taginvoice_productcategory"
        tools:layout_editor_absoluteY="15dp"
        tools:layout_editor_absoluteX="60dp"
        android:paddingRight="10dp" />
</android.support.constraint.ConstraintLayout>